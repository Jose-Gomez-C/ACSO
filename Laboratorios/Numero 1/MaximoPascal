program maximo;
var
        valorUsuario,numeroDos,maximo:real;
begin
        write('Intoduzca el primer numero entero: ');
        readln(valorUsuario);
        write('Intoduzca el segundo numero entero: ');
        readln(numeroDos);
        if valorUsuario>numeroDos then
                begin
                        maximo:=valorUsuario
                end
        else maximo:= numeroDos;
        write('El valor maximo es: ',maximo:0:4);
end.
